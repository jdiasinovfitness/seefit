{
  "name": "seefit",
  "version": "1.0.0",
  "author": "Inovretail",
  "description": "SeeFit Frontend client",
  "scripts": {
    "ng": "ng",
    "start:dev": "ng serve --host=0.0.0.0 --disable-host-check --proxy-config ./src/proxy.conf.js --poll=2000 ",
    "start:tst": "ng serve --proxy-config ./src/proxy.conf.js --poll=2000",
    "start:ios": "npx cap open ios",
    "start:android": "npx cap open android",
    "start:desktop": "cd electron && npm run electron:start-live",
    "start:desktop:build": "cd electron && npm run electron:pack",
    "prebuild": "npm run lint:check",
    "build": "ng build -c production",
    "build:native": "rm -rf ../native/www && npm run build && mv dist ../native/www",
    "watch": "ng build --watch",
    "test": "npm run cypress:run",
    "cypress:open": "npx cypress open",
    "cypress:open:test:component": "npx cypress open --component",
    "cypress:run": "npx cypress run ",
    "cypress:run:test:component": "npx cypress run --component",
    "cypress:run:test:e2e": "npx cypress run --e2e",
    "cypress:chrome": "npx cypress run --browser chrome",
    "lint:check": "prettier --check src/**/*.{js,ts,scss,html,json}",
    "lint:format": "prettier --write src/**/*.{js,ts,css,scss,html,json}",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@capacitor-community/electron": "^4.1.2",
    "@capacitor/android": "^4.7.1",
    "@capacitor/app": "^4.1.1",
    "@capacitor/core": "^4.7.1",
    "@capacitor/haptics": "^4.1.0",
    "@capacitor/ios": "^4.7.1",
    "@capacitor/keyboard": "^4.1.1",
    "@capacitor/status-bar": "^4.1.1",
    "@i9r/ui-components": "^2.0.0",
    "@ionic/angular": "^7.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "ionicons": "^7.1.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular-eslint/builder": "^15.0.0",
    "@angular-eslint/eslint-plugin": "^15.0.0",
    "@angular-eslint/eslint-plugin-template": "^15.0.0",
    "@angular-eslint/schematics": "^15.0.0",
    "@angular-eslint/template-parser": "^15.0.0",
    "@angular/cli": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@angular/language-service": "^15.0.0",
    "@capacitor/cli": "^4.7.1",
    "@ionic/angular-toolkit": "^8.0.0",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "cypress": "^10.8.0",
    "prettier": "^2.8.4",
    "ts-node": "~8.3.0",
    "typescript": "~4.8.4"
  }
}
