{
  "scripts": {
    "ver": "node scripts/versioning.js && cordova prepare",
    "dev:android": "cordova build android && cordova emulate android",
    "run:android": "cordova build android && cordova run android --device",
    "dist:android": "cordova build android --release --buildConfig ./build.json",
    "postdist:android": "ncp ./platforms/android/app/build/outputs/apk/release/app-release.apk ./dist/seeplus.aab",
    "dist:ios": "cordova build ios --device --release --buildConfig ./build.json",
    "postdist:ios": "ncp ./platforms/ios/build/device/seemobile.ipa ./dist/seeplus.ipa"
  },
  "dependencies": {
    "cordova": "^11.0.0",
    "fs-extra": "^10.1.0",
    "ncp": "^2.0.0",
    "replace": "^1.2.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "cordova-android": "^10.1.2",
    "cordova-ios": "^6.2.0"
  },
  "cordova": {
    "platforms": [
      "android",
      "ios"
    ]
  }
}