{
  "scripts": {
    "ver": "node scripts/versioning.js && cordova prepare",
    "dev:android": "cordova build android && cordova emulate android",
    "run:android": "cordova build android && cordova run android --device",
    "dist:android": "cordova build android --verbose --release --buildConfig ./build.json",
    "postdist:android": "ncp ./platforms/android/app/build/outputs/bundle/release/app-release.aab ./dist/seefit.aab",
    "dist:ios": "cordova build ios --device --release --buildConfig ./build.json",
    "postdist:ios": "ncp ./platforms/ios/build/device/seemobile.ipa ./dist/seeplus.ipa"
  },
  "dependencies": {
    "cordova": "^11.0.0",
    "cordova-plugin-androidx-adapter": "^1.1.3",
    "fs-extra": "^10.1.0",
    "ncp": "^2.0.0",
    "replace": "^1.2.2",
    "xml2js": "^0.4.23"
  },
  "cordova": {
    "platforms": [
      "android"
    ]
  },
  "devDependencies": {
    "cordova-android": "^11.0.0"
  }
}